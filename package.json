    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.15",
    "typedoc": "^0.20.23",
    "typescript": "^4.1.3"
